{
  "name": "collabender",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true,
  "scripts": {
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "test-a11y:local": "node scripts/test-accessibility.js --local",
    "test-a11y:live": "node scripts/test-accessibility.js --live",
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "vercel deploy",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "update": "npx npm-check-updates -u && npm install"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.5.5",
    "@builder.io/qwik-city": "^1.5.5",
    "@types/eslint": "7.28.0",
    "@types/node": "^20.12.12",
    "@types/toastify-js": "^1.12.3",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "autoprefixer": "^10.4.19",
    "eslint": "8.57.0",
    "eslint-plugin-qwik": "1.5.5",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "5.4.5",
    "undici": "6.18.1",
    "vercel": "^32.3.0",
    "vite": "5.2.12",
    "vite-tsconfig-paths": "4.3.2"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.9.1",
    "@supabase/supabase-js": "^2.43.4",
    "playwright": "^1.44.1",
    "supabase-auth-helpers-qwik": "^0.0.3",
    "toastify-js": "^1.12.0"
  }
}
